	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#a6a6a680" // comment
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable"
				// "string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#A6A6A6"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#A13030"
			}
		},
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#3061A1"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#A17830"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#A13030"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#5D8547"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.operator.new",
				"keyword.other"
			],
			"settings": {
				"foreground": "#A46B33"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator",
				"keyword.operator.arithmetic",
				"keyword.operator.assignment",
				"keyword.operator.logical"
			],
			"settings": {
				"foreground": "#A6A6A6"
			}
		},
		{
			"name": "Function",
			"scope": [
				"meta.function",
				"meta.function-call",
				"support.function",
				"entity.name.function"
			],
			"settings": {
				"foreground": "#498D99"
			}
		},
		{
			"name": "Type/Class",
			"scope": [
				"meta.class",
				"support.class",
				"entity.name.class",
				"entity.other.inherited-class",
				"meta.type",
				"meta.type.annotation",
				"support.type",
				"entity.name.type",
				"storage.type"
			],
			"settings": {
				"foreground": "#71599E"
			}
		},
		{
			"name": "Property",
			"scope": [
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#4C7399"
			}
		},
		{
			"name": "Constant",
			"scope": [
				"support.constant",
				"constant"
			],
			"settings": {
				"foreground": "#999850"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable",
				"support.variable"
			],
			"settings": {
				"foreground": "#4C7399"
			}
		},
		{
			"name": "Literals",
			"scope": [
				"string",
				"constant"
			],
			"settings": {
				"foreground": "#A39966"
			}
		},
		{
			"name": "Markdown Header",
			"scope": [
				"markup.heading"
			],
			"settings": {
				"foreground": "#5D87A5",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Bold",
			"scope": [
				"markup.bold"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Italic",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markdown Underline",
			"scope": [
				"markup.underline"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markdown List",
			"scope": [
				"markup.list"
			],
			"settings": {
				"foreground": "#A6A6A6"
			}
		}
	]
}